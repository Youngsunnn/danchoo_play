extends ../base

block content
    if errorMessage
        span=errorMessage
    form(method="POST", enctype="multipart/form-data")
        label(for="audios") Upload Files
        input(type="file", id="audios", name="audios")
        input(name="title", placeholder="Title", required, type="text")
        input(name="nextone", placeholder="nextone", required, type="text")
        input(type="submit", value="submit")